#!/usr/bin/env bash

npx textmate-tester \
    --textmateExtension csv \
    --syntax "$FORNIX_FOLDER/autogenerated/csv.tmLanguage.json" \
    --examples "$FORNIX_FOLDER/language_examples/csv" \
    generate-specs --all

npx textmate-tester \
    --textmateExtension tsv \
    --syntax "$FORNIX_FOLDER/autogenerated/tsv.tmLanguage.json" \
    --examples "$FORNIX_FOLDER/language_examples/tsv" \
    generate-specs --all
    
    # --supportSyntaxes '{ "source.YOUR_LANG_EXTENSION_HERE.embedded.macro": "'"$FORNIX_FOLDER"'/autogenerated/YOUR_LANG_EXTENSION_HERE.embedded.macro.tmLanguage.json" }' 